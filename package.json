{
  "name": "confng",
  "version": "1.0.0",
  "description": "Next generation configuration management tool for Node.js",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "prepare": "git config core.hooksPath .githook",
    "check": "biome check",
    "fix": "biome check --write",
    "test": "vitest --run src",
    "build": "tsup",
    "clean": "rm -rf dist",
    "prepack": "tsup"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md", "package.json"],
  "keywords": ["simple", "configuration", "node.js"],
  "author": "Shawn Wen",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.21",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "snakify-ts": "^2.3.0"
  }
}
